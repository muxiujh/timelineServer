@using WxhnecServer.Tools;
@using System.Reflection;
@model WxhnecServer.Logics.TBag
@{
    if (Model.isSub) {
        Layout = null;
    }
}

@foreach (PropertyInfo pro in Model.propertyList) {
    object val = pro.GetValue(Model.row);

    if (THelper.IsVirtual(pro)) {
        if (THelper.HasElement(pro)) {
            Html.RenderAction("AddSub", new { val = val });
        }
    }
    else {
        Html.RenderAction("Field", new { pro = pro, val = val });
    }
}