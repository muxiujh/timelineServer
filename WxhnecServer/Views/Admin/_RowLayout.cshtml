@{ Layout = null; }

<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="keywords" content="">
    <meta name="description" content="">

    <title></title>

    <link id="css_begin" href="@ViewBag.ui/bootstrap3/css/cerulean.bootstrap.min.css?v=10" rel="stylesheet nofollow" type="text/css" />

    <link href="@ViewBag.resourceAdmin/css/master.css?v=10" rel="stylesheet nofollow" type="text/css" />

    <!--[if lt IE 9]>
        <script src="@ViewBag.ui/jquery/html5shiv.min.js"type="text/javascript"></script>
        <script src="@ViewBag.ui/jquery/respond.min.js"type="text/javascript"></script>
        <style>
        .img-thumbnail{max-width: 88%}
        .btn {min-width:60px;}
        .form-horizontal .control-label {text-align: right}
        </style>
    <![endif]-->

    <script type="text/javascript" src="@ViewBag.ui/jquery/jquery-1.9.1.min.js"></script>

</head>




<body id="body">


    <div class="container well-sm">


        <div class="panel panel-default">
            <div class="panel-heading">
                @ViewBag.title
            </div>

            <div class="panel-body">
                <!-- form content -->
                <form action="@ViewBag.UrlSave" style="" class="form-horizontal jh-div-xxl-sub" role="form" method="post" jh_node='form_send'>

                    @RenderBody()

                    <!---------- form-group ---------->
                    <br />
                    <div class="form-group">
                        <div class="col-xs-offset-2 col-xs-5 ">
                            <button type="submit" class="btn btn-primary jh_button" style="width:120px;">提&nbsp;交</button>
                        </div>
                    </div>

                </form>

            </div> <!-- panel-body -->
        </div> <!-- panel -->


    </div><!-- container -->

    <!-------------- jh_template -------------->
    <div id="jh_template" style="display:none">
        <!-- button -->
        <div class="upload_button">
            <div text_loading='正在上传...'>上传图片</div>
            <input type="file" name="file" class="upload_button_file" />
        </div>
        <!-- list -->
        <ul class="upload_ul">
            <li class="upload_li">
                <img class="upload_image img-thumbnail" src="Uploads/Image/loading.gif" />
                <input class="upload_file_id" type='hidden' />
                <input class="upload_file_path" type='hidden' />
                <iframe width="0" height="0" frameborder="0" style="display:none"></iframe>
            </li>
        </ul>
        <!-- form -->
        <form class="upload_form" method="post" enctype="multipart/form-data">
            <input type="text" name="index" />
        </form>
    </div>
    <!-------------- /jh_template -------------->
    <!-------------- jh_upload -------------->
    <script type="text/javascript" src="@ViewBag.ui/jquery/jh_upload.js"></script>
    <script language="javascript">

$(function () {

$(".upload_one").jh_upload({
    validation: {
        itemLimit: 1,
        itemArray: false,
        allowedExtensions:['jpg', 'jpeg', 'png']
    },
    upload_delete: false,
    url: '/Upload/Index'
    //url: '?c=agent&rc=upload&html=1&form=company'
});

});

    </script>
    <!-------------- /jh_upload -------------->


    <script type="text/javascript" src="@ViewBag.ui/bootstrap3/js/bootstrap.js"></script>
    <!-- -->

    <script type="text/javascript" src="@ViewBag.ui/jquery/jquery.form.js"></script>
    <script type="text/javascript" src="@ViewBag.ui/jquery/jh_common.js"></script>
    <script type="text/javascript" src="@ViewBag.ui/jquery/jh_popover.js"></script>
    <script type="text/javascript" src="@ViewBag.ui/jquery/jh_dialog.js"></script>
    <script type="text/javascript" src="@ViewBag.ui/jquery/jh_alert.js"></script>
    <script type="text/javascript" src="@ViewBag.ui/jquery/jh_dropdown.js"></script>
    <script type="text/javascript" src="@ViewBag.ui/jquery/jh_tab.js"></script>
    <script type="text/javascript" src="@ViewBag.ui/jquery/jh_button.js"></script>
    <script type="text/javascript" src="@ViewBag.ui/jquery/jh_node.js"></script>
    <script type="text/javascript" src="@ViewBag.ui/jquery/jh_color.js"></script>
    <script type="text/javascript" src="@ViewBag.ui/jquery/jh_placeholder.js"></script>
    <script type="text/javascript" src="@ViewBag.ui/jquery/jquery.lazyload.min.js"></script>
    <script type="text/javascript" src="@ViewBag.ui/jquery/superslide.2.1.js"></script>

    <!--
    <script type="text/javascript" src="@ViewBag.ui/jquery/jh.js?v=10"></script>
     -->
    <!--
    <script type="text/javascript" src="@ViewBag.ui/jquery/jh_validate.js"></script>
    <script type="text/javascript" src="@ViewBag.ui/jquery/jh_upload.js"></script>
    <script type="text/javascript" src="@ViewBag.ui/jquery/jh_imgcut.js"></script>
    <script type="text/javascript" src="@ViewBag.ui/jquery/jh_waterfall.js"></script>
    <script type="text/javascript" src="@ViewBag.ui/jquery/jh_color_control.js"></script>
    <script type="text/javascript" src="@ViewBag.ui/jquery/jh_editor.js"></script>
    <script type="text/javascript" src="@ViewBag.ui/jquery/jh_editor_form.js"></script>
    -->

    <script type="text/javascript" src="@ViewBag.resourceAdmin/js/event.js?v=10"></script>
    <script type="text/javascript" src="@ViewBag.resourceAdmin/js/global.js?v=10"></script>
</body>
</html>