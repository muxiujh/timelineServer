@using JCore;
@{ Layout = null; }

<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="keywords" content="">
    <meta name="description" content="">

    <title></title>

    <link id="css_begin" href="@ViewBag.ui/bootstrap3/css/cerulean.bootstrap.min.css?v=10" rel="stylesheet nofollow" type="text/css" />

    <link href="@ViewBag.resourceAdmin/../css/global.css?v=10" rel="stylesheet nofollow" type="text/css" />
