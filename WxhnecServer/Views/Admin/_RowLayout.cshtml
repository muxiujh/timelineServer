    <script type="text/javascript" src="@ViewBag.ui/jquery/jh_color.js"></script>
    <script type="text/javascript" src="@ViewBag.ui/jquery/jh_placeholder.js"></script>
    <script type="text/javascript" src="@ViewBag.ui/jquery/jh_uploadMany.js"></script>
    <script type="text/javascript" src="@ViewBag.ui/jquery/jquery.lazyload.min.js"></script>
    <script type="text/javascript" src="@ViewBag.ui/jquery/superslide.2.1.js"></script>

