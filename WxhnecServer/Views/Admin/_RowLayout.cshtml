    <script type="text/javascript" src="@ViewBag.ui/jquery/jquery-1.9.1.min.js"></script>

</head>




<body id="body">


    <div class="container well-sm">


        <div class="panel panel-default">
            <div class="panel-heading">
                @ViewBag.title
            </div>

            <div class="panel-body">
                <!-- form content -->
                <form action="@ViewBag.UrlSave" style="" class="form-horizontal jh-div-xxl-sub" role="form" method="post" jh_node='form_send'>

                    @RenderBody()

                    <!---------- form-group ---------->
                    <br />
                    <div class="form-group">
                        <div class="col-xs-offset-2 col-xs-5 ">
                            <button type="submit" class="btn btn-primary jh_button" style="width:120px;">@G.L["submit"]</button>
                        </div>
                    </div>

                </form>

            </div> <!-- panel-body -->
        </div> <!-- panel -->


    </div><!-- container -->

    <!-------------- jh_uploadMany -------------->
    <script language="javascript">

    $(function () {
        $(".uploadMany").jh_uploadMany();
    });

    </script>
    <!-------------- /jh_uploadMany -------------->


