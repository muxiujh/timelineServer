@{
    Layout = null;
    if(ViewBag.bodyClass == null) {
        ViewBag.bodyClass = "admin_main";
    }
}

<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Admin Center</title>

    <link href="@ViewBag.ui/bootstrap3/css/bootstrap.min.css?v={:C('FORUM_VERSION')}" rel="stylesheet nofollow" type="text/css" />

    <!--[if lt IE 9]>
        <script src="@ViewBag.ui/jquery/html5shiv.min.js"type="text/javascript"></script>
        <script src="@ViewBag.ui/jquery/respond.min.js"type="text/javascript"></script>
    <![endif]-->

    <link href="@ViewBag.ui/bootstrap3/css/global.css?v={:C('FORUM_VERSION')}" rel="stylesheet nofollow" type="text/css" />
    <link href="@ViewBag.resourceAdmin/css/master.css?v={:C('FORUM_VERSION')}" rel="stylesheet nofollow" type="text/css" />

    <script type="text/javascript" src="@ViewBag.ui/jquery/jquery-1.9.1.min.js"></script>
    <script type="text/javascript" src="@ViewBag.ui/bootstrap3/js/bootstrap.js"></script>

</head>

<body class="@ViewBag.bodyClass">

    @RenderBody()

    <script type="text/javascript" src="@ViewBag.ui/jquery/jquery.form.js?v={:C('FORUM_VERSION')}"></script>
    <script type="text/javascript" src="@ViewBag.ui/jquery/jh_common.js?v={:C('FORUM_VERSION')}"></script>
    <script type="text/javascript" src="@ViewBag.ui/jquery/jh_popover.js?v={:C('FORUM_VERSION')}"></script>
    <script type="text/javascript" src="@ViewBag.ui/jquery/jh_dialog.js?v={:C('FORUM_VERSION')}"></script>
    <script type="text/javascript" src="@ViewBag.ui/jquery/jh_alert.js?v={:C('FORUM_VERSION')}"></script>
    <script type="text/javascript" src="@ViewBag.ui/jquery/jh_dropdown.js?v={:C('FORUM_VERSION')}"></script>
    <script type="text/javascript" src="@ViewBag.ui/jquery/jh_tab.js?v={:C('FORUM_VERSION')}"></script>
    <script type="text/javascript" src="@ViewBag.ui/jquery/jh_button.js?v={:C('FORUM_VERSION')}"></script>
    <script type="text/javascript" src="@ViewBag.ui/jquery/jh_node.js?v={:C('FORUM_VERSION')}"></script>
    <script type="text/javascript" src="@ViewBag.ui/jquery/jh_color.js?v={:C('FORUM_VERSION')}"></script>
    <script type="text/javascript" src="@ViewBag.ui/jquery/jh_placeholder.js?v={:C('FORUM_VERSION')}"></script>
    <script type="text/javascript" src="@ViewBag.ui/jquery/jquery.lazyload.min.js?v={:C('FORUM_VERSION')}"></script>

    <script type="text/javascript" src="@ViewBag.resourceAdmin/js/event.js?v={:C('FORUM_VERSION')}"></script>
    <script type="text/javascript" src="@ViewBag.resourceAdmin/js/global.js?v={:C('FORUM_VERSION')}"></script>

</body>
</html>

